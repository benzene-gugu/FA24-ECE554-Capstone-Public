set_instance_assignment -name IO_STANDARD "3.3-V LVTTL" -to MOSI0[0]    # which is also MISO1
set_instance_assignment -name IO_STANDARD "3.3-V LVTTL" -to MOSI0[2]


set_instance_assignment -name IO_STANDARD "3.3-V LVTTL" -to MOSI1[3]
