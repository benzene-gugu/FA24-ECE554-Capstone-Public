set_instance_assignment -name IO_STANDARD "3.3-V LVTTL" -to MOSI1[0]    # which is also MISO1
set_instance_assignment -name IO_STANDARD "3.3-V LVTTL" -to MOSI1[2]




set_instance_assignment -name IO_STANDARD "3.3-V LVTTL" -to MOSI2[3]
set_instance_assignment -name IO_STANDARD "3.3-V LVTTL" -to SS2_n
set_instance_assignment -name IO_STANDARD "3.3-V LVTTL" -to SCLK2
