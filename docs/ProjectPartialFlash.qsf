
#============================================================
# GPIO_0, GPIO_0 connect to GPIO Default
#============================================================
set_instance_assignment -name IO_STANDARD "3.3-V LVTTL" -to MOSI0[3]
set_instance_assignment -name IO_STANDARD "3.3-V LVTTL" -to SS0_n
set_instance_assignment -name IO_STANDARD "3.3-V LVTTL" -to SCLK0
